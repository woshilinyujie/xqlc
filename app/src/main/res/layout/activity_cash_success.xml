<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_invest_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.AppBarOverlay"
            app:elevation="0px">

            <android.support.v7.widget.Toolbar
                android:id="@+id/project_invest_toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorWhite"
                app:popupTheme="@style/AppTheme.PopupOverlay">

                <com.rongxun.xqlc.UI.IconFontTextView
                    android:id="@+id/cash_success_bank_back"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="left"
                    android:gravity="center"
                    android:text="@string/angle_left"
                    android:textColor="@color/colorBlack"
                    android:textSize="24dp"

                    />

                <TextView
                    android:id="@+id/project_invest_toolbar_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="5dp"
                    android:text="提现结果"
                    android:textColor="@color/colorBlack"
                    android:textSize="18dp"
                    android:visibility="visible" />


            </android.support.v7.widget.Toolbar>
        </android.support.design.widget.AppBarLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f5f5f5" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="280dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/button_background_top_up"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="190dp"
                android:layout_height="120dp"
                android:layout_marginTop="20dp"
                android:background="@mipmap/invest_success" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="提现申请成功，等待处理..."
                android:textColor="#333333"
                android:textSize="20dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"
                android:text="到账时间说明："
                android:textColor="#fa5454"
                android:textSize="15dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="3dp"
                android:text="工作日下午16:00之前申请的提现会在当日18:00左右到账，16:00后申请的提现将在下个工作日到账。周末及法定假日提现，将在下个工作日到帐。"
                android:textSize="13dp" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f5f5f5" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="1dp"
            android:background="@color/colorWhite"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/invest_result_project"
                android:layout_alignBottom="@+id/invest_result_project"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:text="提现至"
                android:textColor="#333333"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/cash_success_bank_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="--"
                android:textColor="#444444"
                android:textSize="15dp" />

            <ImageView
                android:id="@+id/cash_success_bank_iv"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginRight="8dp"
                android:layout_toLeftOf="@id/cash_success_bank_name" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colorWhite"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:text="提现金额"
                android:textColor="#333333"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/cash_success_bank_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="--"
                android:textColor="#fa5454"
                android:textSize="15dp" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colorWhite"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:text="提现手续费"
                android:textColor="#333333"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/cash_success_bank_cash_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="--"
                android:textColor="#fa5454"
                android:textSize="15dp" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/colorWhite"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:text="实际到账金额"
                android:textColor="#333333"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/cash_success_bank_arrive_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:text="--"
                android:textColor="#fa5454"
                android:textSize="15dp" />


        </RelativeLayout>

        <Button

            android:id="@+id/cash_success_bank_button"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/button_background_normal"
            android:text="确定"
            android:textColor="#ffffff"
            android:textSize="16dp" />

        <TextView
            android:layout_marginBottom="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="如有疑问请致电金储宝客服400-033-3113"
            android:textColor="#999999"
            android:textSize="14dp" />
    </LinearLayout>

</ScrollView>
